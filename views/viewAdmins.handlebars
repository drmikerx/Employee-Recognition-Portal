<div class="container" style="margin-top:100px; margin-bottom:50px">
    <h1 class="text-center">View/Edit/Delete Current Admins</h1>
</div>
<table class="table table-striped table-hover">
    <thead class="thead-dark text-center">
        <tr>
            <th scope="col">Username</th>
            <th scope="col">Time of Account Creation</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody class="text-center">
        {{#each admins}}
        <tr>
            <td>{{Username}}</td>
            <td>{{TimeCreated}}</td>
            <td>
                <button type="button" class="btn btn-primary" onclick="window.location.href = '/editAdmin/{{id}}'">Edit</button>
                <button type="button" class="btn btn-danger" onclick="deleteAdmin({{id}})">Delete</button>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>